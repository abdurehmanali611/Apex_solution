generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mysql"
}

model Users {
  id Int @id @default(autoincrement())
  username String
  password String
}

model HeroFooter {
  id Int @id @default(autoincrement())
  name String
  amount Int
}

model Services {
  id Int @id @default(autoincrement())
  icon String
  title String
  description String
}

model PortFolio {
  id Int @id @default(autoincrement())
  title String
  description String
  link String
  type String
  duration Int
  image String
  version Int
  special Boolean
}

model Blogs {
  id Int @id @default(autoincrement())
  title String
  description String
  image String
  source String
  date DateTime
  link String
}

model Testimonials {
  id Int @id @default(autoincrement())
  name String
  profession String
  image String
  content String
  rating Int
}

model Partner {
  id Int @id @default(autoincrement())
  title String
  image String
  description String
}

model Teams {
  id Int @id @default(autoincrement())
  name String
  position String
  image String
  title String
  description String
  facebook String
  instagram String
  linkedin String
  telegram String
}

model Contacts {
  id Int @id @default(autoincrement())
  Full_Name String
  Email String
  Subject String
  Message String
}
